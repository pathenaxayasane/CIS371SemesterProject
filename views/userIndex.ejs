<body>
    <h1>Listing Bugs</h1>


    
    </div>
    <table>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Thumbnail</th>
            
        </tr>

        <% users.forEach( (user) => { %>
            <tr>
                <td>
                    <%= user.fname %> 
                </td>
                <td>
                    <%= user.lname %>
                </td>
                <td>
                    <%= user.email %>
                </td>
                <td>
                    <%= user.thubnail %>
                </td>
                
                <td>
                    <a href="/users/<%= user.id %>">Show</a>
                    <a href="/users/<%= user.id %>/edit">Edit</a>
                    <a href="/users/<%= user.id %>/delete">Delete</a>
                </td>
                <% }) %>
            </tr>
    </table>
</body>

